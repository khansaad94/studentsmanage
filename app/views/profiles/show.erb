    $(document).ready(function () {
        $(".menu-icon").click(function () {
            $(this).next('ul').show();
            //$('').toggle("slide", { direction: "up" }, 1000);
        });
    });
    function OpenInNewTab(url) {
        if (url.indexOf("http://") < 0) {
            url = "http://" + url;
        }
        var win = window.open(url, '_blank');
        win.focus();
    }
</script>
